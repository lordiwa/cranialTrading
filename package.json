{
  "name": "cranial-trading",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:report": "eslint src --format html --output-file eslint-report.html",
    "lint:strict": "eslint src --max-warnings 0 --rule '{\"@typescript-eslint/no-explicit-any\": \"error\", \"no-console\": \"error\"}'",
    "prebuild": "npm run type-check && npm run lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "presubmit": "npm run type-check && npm run lint && npm run test:integration",
    "verify": "npm run presubmit",
    "prepare": "husky"
  },
  "dependencies": {
    "driver.js": "^1.4.0",
    "firebase": "^11.0.0",
    "firebase-admin": "^13.6.0",
    "lodash-es": "^4.17.22",
    "pinia": "^2.2.0",
    "vue": "^3.5.0",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^17.0.0",
    "happy-dom": "^20.5.0",
    "husky": "^9.1.7",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.53.0",
    "vite": "^6.0.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^2.1.0"
  }
}
